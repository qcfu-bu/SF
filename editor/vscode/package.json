{
  "name": "sf-lang",
  "displayName": "SF Language Support",
  "description": "Syntax highlighting and language support for the SF programming language",
  "version": "0.1.0",
  "publisher": "sf-lang",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sf",
        "aliases": [
          "SF",
          "sf"
        ],
        "extensions": [
          ".sf"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/sf-icon.png",
          "dark": "./icons/sf-icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "sf",
        "scopeName": "source.sf",
        "path": "./syntaxes/sf.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sf-lang"
  },
  "license": "MIT"
}
